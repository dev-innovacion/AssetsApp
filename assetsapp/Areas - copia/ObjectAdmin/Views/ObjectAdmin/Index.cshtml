        function getids() {
            var table = jQuery("#objectsTab #objectsTab_rvtable").dataTable()
            var trs = table.fnGetNodes()
            var checks = [];
            jQuery.each(trs, function () {
                 
                if (jQuery(this).find("input:not(:disabled)").is(":checked")) {
                    checks.push(jQuery(this).data("id"))
                }
            });
           
            return checks;
        }
        function getidsbystatus(status) {
            var table = jQuery("#objectsTab #objectsTab_rvtable").dataTable()
            var trs = table.fnGetNodes()
            var checks = [];
            jQuery.each(trs, function () {

                if (jQuery(this).find("input:not(:disabled)").is(":checked")) {
                    if(jQuery(this).data("systemstatus")==status)
                       checks.push(jQuery(this).data("id"))
                }
            });

            return checks;
        }
