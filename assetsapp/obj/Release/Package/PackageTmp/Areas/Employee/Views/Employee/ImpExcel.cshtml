@{Layout = null;}


@model List<List<string>>
@{
    ViewBag.Title = "Table Excel";
    int count = 0;
    int index = 0;
    List<string> listheads = new List<string>();
    int idselect = 0;
    string cad = "";
    int selectvalue = 0;
    var file = ViewData["Filerequest"];
    string filename = Request.Files["file"].FileName;
    filename = filename.Split('.')[0];
    int nameheadx = 0;



}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="icon-remove" style="margin-top: 10px; margin-right: 10px"></i></button>

    <h3 class="modal-header-text">@filename</h3>
</div>
<div class="modal-body" id="ExcelContent" style="padding-top: 0px;">
    <div class="row-fluid">
        <div class="span5">

            @foreach (var item in Model)
            {

                foreach (var td in item)
                {
                    if (td == "")
                    {
                        listheads.Add("Undefined" + nameheadx);
                        nameheadx++;
                    }
                    else
                    {
                        listheads.Add(td);
                    }

                }



                break;

            }
            <h2>Campos del Empleado:</h2>
            <br />
            <div style="overflow:auto;max-height:540px; margin-top: -30px;">
                <!-- Campo del Empleado -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">ID Empleado:</label></div>
                    <div class="span7">
                        <select id="employee1" class="chosen" data-required="1" data-name="employee">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Nombre -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Nombre :</label></div>
                    <div class="span7">
                        <select id="name1" class="chosen" data-required="1" data-name="name">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Apellido Paterno -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Apellido Paterno :</label></div>
                    <div class="span7">
                        <select id="lastname1" class="chosen" data-required="1" data-name="lastname">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Apellido Materno -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Apellido Materno :</label></div>
                    <div class="span7">
                        <select id="motherlastname1" class="chosen" data-required="0" data-name="motherlastname">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Perfil -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Perfil :</label></div>
                    <div class="span7">
                        <select id="profileId1" class="chosen" data-required="1" data-name="profileId">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Tipo de Empleado -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Tipo de Empleado :</label></div>
                    <div class="span7">
                        <select id="type1" class="chosen" data-required="0" data-name="type">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
                <!-- Campo del Área -->
                <div class="row-fluid">
                    <div class="span5"> <label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">Área :</label></div>
                    <div class="span7">
                        <select id="area1" class="chosen" data-required="0" data-name="area">
                            @foreach (string opt in listheads)
                            {
                                if (index == 0)
                                {
                                    <option value="no">Elige una opcion</option>
                                    index++;
                                }
                                <option value="@selectvalue">@opt</option>
                                selectvalue++;
                            }
                            @{index = 0; selectvalue = 0;}
                        </select>
                    </div>
                </div>
              


                <div class="row-fluid">
                  
                    <input name='uploads[]' class="uploadFile" id="uploadFiles" type="file" />
                    
                </div>

                <br />
                @foreach (var datacateg in (List<List<string>>)ViewData["categoriedata"])
                {

                    index = 0;
                    selectvalue = 0;
                    List<string> listrowscat = new List<string>();
                    foreach (string x in datacateg)
                    {
                        listrowscat.Add(x.ToString());

                    }
                    string[] arrayf = listrowscat.ToArray();

                    <div class="row-fluid">

                        <div class="span5"><label style="font-size:16px;padding:5% 0% 0% 0%;float:right;">@arrayf[1] :</label></div>
                        <div class="span7 no-search">
                            <select id="@idselect" data-required="@arrayf[2]" data-name="@arrayf[1]" class="chosen">
                                @foreach (string opt in listheads)
                                {
                                    if (index == 0)
                                    {
                                        <option value="no">Elige una opcion</option>
                                        index++;
                                    }
                                    <option value="@selectvalue">@opt</option>
                                    selectvalue++;
                                }
                            </select>

                        </div>
                    </div>
                    <br />
                                idselect++;
                }
            </div>
        </div>

        <div class="span7 ">
            <h2>  Datos A Importar :</h2>
            <div style="overflow:auto;max-height:480px;">
                <table id="exceldatatable" class="table table-striped table-hover ">


                    @foreach (var item in Model)
                    {
                        nameheadx = 0;
                        <thead>
                            <tr>
                                <th class="table-icon hidden-480 gray" data-head="checkall"><input type="checkbox" name="selectall" class="uniform" checked></th>
                                @foreach (var td in item)
                                {
                                    string value = td;
                                    if (td == "")
                                    {
                                        value = "Undefined" + nameheadx;
                                        nameheadx++;
                                    }
                                    <th class="hidden-480 gray" style="color:white;" data-head="@value">@value</th>
                                }

                            </tr>
                        </thead>
                                count++;
                                break;

                    }

                    <tbody>


                        @foreach (var item in Model)
                        {


                            if (count > 1)
                            {
                                <tr>
                                    <td class="table-icon hidden-480">
                                        <input type="checkbox" name="selectdate" class="uniform" checked>
                                    </td>
                                    @foreach (var td in item)
                                    {

                                        <td class="hidden-480" data-td="@td">@td</td>

                                    }
                                </tr>

                            }
                            count++;

                        }
                    </tbody>



                </table>
            </div>
        </div>

    </div>
</div>
@{

    string fileLocation2 = ViewData["Filelocation"].ToString();

    if (System.IO.File.Exists(fileLocation2))
    {

        System.IO.File.Delete(fileLocation2);
    }

}
<div class="modal-footer">
    <input value="Guardar" class="btn blue" type="submit" id="saveImport">
    <input value="Cancelar" class="btn grey" type="button" id="delete_cancel_buttonx">
</div>


